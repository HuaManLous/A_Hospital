<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%>
<%
    String path=request.getContextPath()+"/";
%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
<base href="<%=path%>">
    <title>修改医生---2015</title>
    <link rel="stylesheet" type="text/css" href="Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="Css/style.css" />
    <script type="text/javascript" src="Js/jquery.js"></script>
    <script type="text/javascript" src="Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="Js/bootstrap.js"></script>
    <script type="text/javascript" src="Js/ckform.js"></script>
    <script type="text/javascript" src="Js/common.js"></script>
    <script type="text/javascript" src="Js/ckeditor/ckeditor.js"></script>

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="Doctor/Query";
		 });
    });
    </script>
</head>
<body>
<form action="Doctor/Update?id=${doctor_CS.id }" method="post" class="definewidth m20">
<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">姓名</td>
        <td><input type="text" name="name" value="${doctor_CS.name }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件类型</td>
        <td>
           <select name="certificate_type">
             <c:forEach items="${certificateType }" var="certificateType">
                 <option value="${certificateType.id }" ${certificateType.id==doctor_CS.certificate_type?'selected':'' }>${certificateType.name }</option>
             </c:forEach>
           </select>
       </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件号</td>
        <td><input type="text" name="ID_num" value="${doctor_CS.ID_num }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">手机</td>
        <td><input type="text" name="phone" value="${doctor_CS.phone }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">座机</td>
        <td><input type="text" name="special_plane" value="${doctor_CS.special_plane }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">性别</td>
        <td>
          <input type="radio" name="sex" value="1" ${doctor_CS.sex==1?'checked':'' }/>男&nbsp;&nbsp;&nbsp;
          <input type="radio" name="sex" value="0" ${doctor_CS.sex==0?'checked':'' }/>女</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">出生年月</td>
        <td><input type="text" name="birther" value="${doctor_CS.birther }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">年龄</td>
        <td><input type="text" name="age" value="${doctor_CS.age }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">电子邮箱</td>
        <td><input type="text" name="email" value="${doctor_CS.email }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">所属科室</td>
        <td>
           <select name="section_id">
              <c:forEach items="${section }" var="section">
                 <option value="${section.id }" ${section.id==doctor_CS.section_id?'selected':'' }>${section.secco_name }</option>
              </c:forEach>
           </select>
       </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">学历</td>
        <td>
           <!-- 1.本科2.专科3.博士4.博士后 其他都是家里蹲 -->
           <select name="education">
              <option value="1" ${doctor_CS.education==1?'selected':'' }>专科</option>
              <option value="2" ${doctor_CS.education==2?'selected':'' }>本科</option>
              <option value="3" ${doctor_CS.education==3?'selected':'' }>博士</option>
              <option value="4" ${doctor_CS.education==4?'selected':'' }>博士后</option>
              <option value="5" ${doctor_CS.education==5?'selected':'' }>家里蹲</option>
           </select>
        </td>
    </tr>
	<tr>
        <td width="10%" class="tableleft">备注</td>
        <td><textarea name="noted">${doctor_CS.noted }</textarea></td>
	</tr>
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
			</center>
		</td>
    </tr>
</table>
